  "keywords": [
    "emgage",
    "engage-ui",
    "react",
    "components",
    "component library"
  ],
  "main": "lib/",
  "types": "./types/src/components/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "node node_modules/tslint/bin/tslint 'src/**/*.{ts,tsx}' --config tslint.json --exclude \"src/**/*.{js,jsx}\" --project tsconfig.json",
    "lint-fix": "node node_modules/tslint/bin/tslint --fix 'src/**/*.{ts,tsx}' --config tslint.json --exclude \"src/**/*.{js,jsx}\" --project tsconfig.json",
    "lint:docs": "node node_modules/tslint/bin/tslint 'docs/**/*.{ts,tsx}' --config tslint.json --exclude \"docs/**/*.{js,jsx}\" --project tsconfig.json",
    "lint-fix:docs": "node node_modules/tslint/bin/tslint --fix 'docs/**/*.{ts,tsx}' --config tslint.json --exclude \"docs/**/*.{js,jsx}\" --project tsconfig.json",
    "test": "node node_modules/jest/bin/jest.js --config ./config/jest/config.json",
    "test-debug": "node --inspect-brk ./node_modules/.bin/jest -i --config ./config/jest/config.json",
    "check": "npm-run-all lint build-dev test",
    "build-dev": "node node_modules/webpack/bin/webpack.js && node node_modules/webpack/bin/webpack.js --config webpack.window.config.js",
    "build-release": "node node_modules/webpack/bin/webpack.js -p && node node_modules/webpack/bin/webpack.js --config webpack.window.config.js -p",
    "example": "webpack-dev-server --config webpack.example.config --progress",
    "docs": "webpack-dev-server --config webpack.docs.config",
    "precommit-msg": "echo 'Running linting and testing checks...' && exit 0",
    "travis-deploy-once": "travis-deploy-once",
    "commit": "node node_modules/semantic-git-commit-cli/dest/cli.js",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "flow",
      "shopify/node"
    ]
  },
  "files": [
    "src",
    "lib",
    "themes",
    "types"
  ],
  "sgc": {
    "lowercaseTypes": true
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "lint:docs"
  ],
  "devDependencies": {
